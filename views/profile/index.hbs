<div>
    <h1>PROFILES</h1>
</div>

{{#each links}}
<div class="row">
    <div class="col s12 m6">
        <div class="card blue-grey darken-1">
            <div class="card-content white-text">
                <a href={{_id}}><span class="card-title"> {{name}} </span></a>
                <p>{{content}}</p>
            </div>
            <div class="card-action">
                <a href={{link}}>Go to Website</a>
            </div>
        </div>
    </div>
</div>


{{/each}}