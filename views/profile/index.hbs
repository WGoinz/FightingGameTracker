<div>
    <h1 class="no-margin bottom-margin">PROFILES</h1>
</div>

<div class="flex">
    {{#each profile}}
    <div class="profile flex-column">
        <h3>{{username}}</h3>
        <img src="https://i.imgur.com/cvpuu7I.png" alt="profile pic">
        <button class="pure-button pure-button-primary"><a href="{{_id}}">VIEW PROFILE</a></button>
    </div>
    {{/each}}
</div>